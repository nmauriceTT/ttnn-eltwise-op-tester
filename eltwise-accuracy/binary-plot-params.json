{
    "groups": [
        {
            "id": "ulp_plots",
            "default_params": {
                "type": "heatmap",
                "dtype": "bfloat16",
                "aname": "a",
                "bname": "b",
                "valuename": "max_ulp_error",
                "ynames": [
                    ["max_ulp_error", "", "-"]
                ],
                "plot_params": {
                    "title": "ULP Error of ttnn.{} against torch\n[bfloat16]",
                    "colormap": [
                        {"threshold": 0, "color": "#2E8B57"},
                        {"threshold": 2, "color": "#FFD700"},
                        {"threshold": 5, "color": "#FF6347"},
                        {"threshold": 10, "color": "#8B0000"}
                    ],
                    "alim": [-1e9, 1e9],
                    "blim": [-1e9, 1e9],
                    "ascale": "symlog",
                    "bscale": "symlog"
                }
            },
